{
  "manifest_version": 2,
  "name": "Source Quicklinks",
  "version": "1.5",
  "description": "Allows quick jumping between Chrome, Blink, WebKit, V8, etc. source links",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "js/closure-lite.js",
      "js/link.js",
      "js/tab-state.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icon19.png",
    "default_title": "Source Quicklinks",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "webNavigation"
  ],
  "omnibox": {
    "keyword" : "sql"
  }
}
